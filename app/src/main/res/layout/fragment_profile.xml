<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.ProfileFragment">


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/phoneNumberPromptTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="32dp"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:fontFamily="@font/nunito_sans_extrabold"
        android:text="Profile"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/paymentDateTV"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginVertical="32dp"
        android:scaleType="centerCrop"
        android:src="@drawable/image_man"
        app:layout_constraintEnd_toEndOf="@+id/phoneNumberPromptTV"
        app:layout_constraintStart_toStartOf="@+id/phoneNumberPromptTV"
        app:layout_constraintTop_toBottomOf="@+id/phoneNumberPromptTV"
        app:shapeAppearanceOverlay="@style/RoundShapeAppearance" />


    <ImageView
        android:id="@+id/uploadIV"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/ic_camera"
        android:layout_marginTop="60dp"
        app:layout_constraintBottom_toBottomOf="@+id/paymentDateTV"
        app:layout_constraintEnd_toEndOf="@+id/paymentDateTV"
        app:layout_constraintTop_toTopOf="@+id/paymentDateTV" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/materialCardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="32dp"
        android:layout_marginVertical="24dp"
        app:cardCornerRadius="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/paymentDateTV"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/profile_details_bg">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/mainLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingHorizontal="8dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="1.0"
                    tools:context=".ui.screens.TransactionDetailActivity">


                    <TextView
                        android:id="@+id/vehicleColorTV"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:maxLines="1"
                        android:textAlignment="viewEnd"
                        android:textColor="@color/black"
                        app:layout_constraintBottom_toBottomOf="@+id/vehicleColorLabelTV"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/vehicleColorLabelTV"
                        app:layout_constraintTop_toTopOf="@+id/vehicleColorLabelTV"
                        tools:text="Matte Black" />

                    <TextView
                        android:id="@+id/vehicleLicenseLabelTV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="32dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:text="Vehicle License :"
                        android:textColor="@color/black"
                        app:layout_constraintEnd_toStartOf="@+id/vehicleLicenseTV"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/vehicleLicenseTV"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:maxLines="1"
                        android:textAlignment="viewEnd"
                        android:textColor="@color/black"
                        app:layout_constraintBottom_toBottomOf="@+id/vehicleLicenseLabelTV"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/vehicleLicenseLabelTV"
                        app:layout_constraintTop_toTopOf="@+id/vehicleLicenseLabelTV"
                        tools:text="IMO511EU" />

                    <TextView
                        android:id="@+id/vehicleRegistrationLabelTV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:text="Vehicle Reg :"
                        android:textColor="@color/black"
                        app:layout_constraintEnd_toStartOf="@+id/vehicleRegistrationTV"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/vehicleLicenseLabelTV" />

                    <TextView
                        android:id="@+id/vehicleRegistrationTV"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:maxLines="1"
                        android:textAlignment="viewEnd"
                        android:textAllCaps="true"
                        android:textColor="@color/black"
                        app:layout_constraintBottom_toBottomOf="@+id/vehicleRegistrationLabelTV"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/vehicleRegistrationLabelTV"
                        app:layout_constraintTop_toTopOf="@+id/vehicleRegistrationLabelTV"
                        tools:text="AX364NJO" />

                    <TextView
                        android:id="@+id/vehicleColorLabelTV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:text="Vehicle Color :"
                        android:textColor="@color/black"
                        app:layout_constraintEnd_toStartOf="@+id/vehicleColorTV"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/vehicleRegistrationLabelTV" />

                    <TextView
                        android:id="@+id/lastPaymentDateLabelTV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="32dp"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:text="Last Pay Date :"
                        android:textColor="@color/black"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/lastPaymentDateTV"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/driverLabelTV" />

                    <TextView
                        android:id="@+id/lastPaymentDateTV"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:maxLines="1"
                        android:textAlignment="viewEnd"
                        android:textColor="@color/blue"
                        app:layout_constraintBottom_toBottomOf="@+id/lastPaymentDateLabelTV"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/lastPaymentDateLabelTV"
                        app:layout_constraintTop_toTopOf="@+id/lastPaymentDateLabelTV"
                        tools:text="01/12/22" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.5" />

                    <TextView
                        android:id="@+id/driverLabelTV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:text="Driver :"
                        android:textColor="@color/black"
                        app:layout_constraintEnd_toStartOf="@+id/driverTV"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/vehicleTypeLabelTV" />

                    <TextView
                        android:id="@+id/driverTV"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:maxLines="1"
                        android:textAlignment="viewEnd"
                        android:textColor="@color/black"
                        app:layout_constraintBottom_toBottomOf="@+id/driverLabelTV"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/driverLabelTV"
                        app:layout_constraintTop_toTopOf="@+id/driverLabelTV"
                        tools:text="Dipo" />

                    <TextView
                        android:id="@+id/vehicleTypeLabelTV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="8dp"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:text="Vehicle Type :"
                        android:textColor="@color/black"
                        app:layout_constraintEnd_toStartOf="@+id/vehicleTypeTV"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/vehicleColorLabelTV" />

                    <TextView
                        android:id="@+id/vehicleTypeTV"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:maxLines="1"
                        android:textAlignment="viewEnd"
                        android:textColor="@color/black"
                        app:layout_constraintBottom_toBottomOf="@+id/vehicleTypeLabelTV"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/vehicleTypeLabelTV"
                        app:layout_constraintTop_toTopOf="@+id/vehicleTypeLabelTV"
                        tools:text="Mercedes Benz" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.card.MaterialCardView>


</androidx.constraintlayout.widget.ConstraintLayout>